<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="teacher" class="com.sailfish.learnspring.hook.Teacher">
        <property name="name" value="橙子"/>
        <property name="age" value="18"/>
        <property name="smoking" value="true"/>
     </bean>

    <bean id="teacherDefinition" class="com.sailfish.learnspring.beanFactoryPostProcessor.ChangeTeacherFactoryPostProcessor"/>



</beans>